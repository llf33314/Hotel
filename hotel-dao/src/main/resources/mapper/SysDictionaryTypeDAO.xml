<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gt.hotel.dao.SysDictionaryTypeDAO">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.gt.hotel.entity.SysDictionaryType">
		<id column="id" property="id" />
		<result column="dict_name" property="dictName" />
		<result column="dict_status" property="dictStatus" />
		<result column="dict_remark" property="dictRemark" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, dict_name AS dictName, dict_status AS dictStatus, dict_remark AS dictRemark
    </sql>

</mapper>
