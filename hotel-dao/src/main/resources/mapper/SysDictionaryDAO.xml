<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gt.hotel.dao.SysDictionaryDAO">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.gt.hotel.entity.SysDictionary">
        <id column="id" property="id"/>
        <result column="dict_type_id" property="dictTypeId"/>
        <result column="dict_code" property="dictCode"/>
        <result column="dict_name" property="dictName"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, dict_type_id AS dictTypeId, dict_code AS dictCode, dict_name AS dictName
    </sql>

</mapper>
